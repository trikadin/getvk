{
  "manifest_version": 2,
  "name": "getvk",
  "description": "",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.vk.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
